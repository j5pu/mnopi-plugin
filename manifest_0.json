{
  "name": "keyhole",
  "version": "0.1",
  "description": "It's a secret, so don't tell anyone.",
  "background": {
    "page": "background.html"
  },
  "browser_action" :
  {
    "default_icon" : "icon.png",
    "default_popup" : "popup.html"
  },
  "permissions": ["tabs", "activeTab", "http://*/*", "https://*/*", "history", "background", "management"],
  
  "icons" : {
    "48" : "icon48.png",
    "128" : "icon128.png"
  },
  "manifest_version": 2
}
